algoritmo "Exiba o Maior"

var

n1, n2: inteiro

inicio
     escreva("digite um número: ")
     leia(n1)
     escreva("digite o segundo número: ")
     leia(n2)
     se(n1>n2)entao
      escreva("O numero maior é: ", n1)
      senão
       escreva("O numero maior é: ", n2)
     fimse
     

fimalgoritmo