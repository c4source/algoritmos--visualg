algoritmo "semnome"


var
nome: caractere
salario: real
inicio
      escreva("digite seu nome: ")
      leia(nome)
      salario <- (1850.45)
      escreval("nome de funcionário: ", nome)
      escreval("Salário: R$", salario)
  escreva("O funcionário ", nome, " tem um salário de", "R$",salario," em junho")
      


fimalgoritmo