algoritmo "sucessor_antecessor"

var
n, a, s: inteiro

inicio
      escreva("número: ")
      leia(n)
      a <-(n-1)
      s <-(n+1)
        escreval("O antecessor de",n , " é",a)
        escreva("O sucessor de",n , " é",s)
        
// subtrair o número escolhido por 1 = antecessor
// Adicionar 1 ao número escolhido = sucessor




fimalgoritmo