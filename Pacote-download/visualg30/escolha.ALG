algoritmo "exercicio 2"
var
nome: caractere
senha: inteiro

inicio
      escreva("Nome de utilizador: ")
      leia(nome)
      escreva("Senha da palavra passe: ")
      leia(senha)
      se nome = "admin" e senha = "1234" entao
         escreval("Acesso permitido!")
      senao
         escreval("Acesso negado")
      fimse



fimalgoritmo